{
  "name": "@smiley-face-game/api",
  "version": "0.0.0",
  "author": "SirJosh3917 <SirJosh3917@users.noreply.github.com>",
  "license": "MIT",
  "main": "src/index.ts",
  "directories": {
    "src": "src"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "mocha --require @swc-node/register 'src/**/*.test.ts'",
    "build": "pika build && npx rollup -c rollup.config.js"
  },
  "dependencies": {
    "computed-types": "^1.4.0",
    "cross-fetch": "^3.0.6",
    "isomorphic-ws": "^4.0.1"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "rollup": "^2.33.3",
    "pika-plugin-minify": "^0.1.0",
    "pika-plugin-package.json": "^1.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.5.0"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "pika-plugin-package.json",
        {
          "*files": [
            "+dist.umd.min.js"
          ],
          "+unpkg": "dist.umd.min.js"
        }
      ],
      [
        "./pika-plugin-minify.js"
      ]
    ]
  }
}
