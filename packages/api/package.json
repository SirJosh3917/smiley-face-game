{
  "name": "@smiley-face-game/api",
  "version": "0.0.0",
  "license": "MIT",
  "directories": {
    "lib": "./dist"
  },
  "scripts": {
    "build-api": "concurrently \"yarn build:server\" \"yarn build:client\"",
    "build:server": "swc -d dist/cjs -C module.type=commonjs ./src/",
    "build:client": "swc -d dist/es -C module.type=commonjs ./src/"
  },
  "dependencies": {
    "computed-types": "^1.1.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.27",
    "@swc/core": "^1.2.34",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concurrently": "^5.3.0",
    "ttypescript": "^1.5.10"
  }
}
